
<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="#">Tablolar</a></li>
        <li class="breadcrumb-item active" aria-current="page">Tedarikci</li>
    </ol>
</nav>
<div >
    <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#hamMaddeModal">
        Tedarikci Ekle
    </button>

    <!-- Ham Madde Ekle Modal -->
    <div class="modal fade" id="hamMaddeModal" tabindex="-1" role="dialog" aria-labelledby="hamMaddeModalTitle" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLongTitle">Ham Madde Ekle</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <%include ../form/tedarikci.ejs%>
                </div>

                <div class="card-footer">
                    <button form="form"  type="submit" class="btn btn-primary">Kaydet</button>
                </div>
            </div>
        </div>
    </div>
</div>
<br>

<div>
    <table class="table">
        <thead>
        <tr>
            <th>Ad</th>
            <th>Telefon</th>
            <th>Mail</th>
        </tr>
        </thead>

        <tbody>
            <%results.forEach(function (item) { %>
               <tr>
                   <td><%=item.ad%></td>
                   <td><%=item.tel%></td>
                   <td><%=item.mail%></td>
               </tr>
            <% })%>
        </tbody>
    </table>
</div>
